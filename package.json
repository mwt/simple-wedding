{
    "title": "Grayscale",
    "name": "startbootstrap-grayscale",
    "version": "7.0.6",
    "scripts": {
        "build": "npm run clean && npm run build:scss && npm run build:assets && npm run build:pug",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "purgecss": "npm run build && purgecss -v -keyframes -o ./dist/css/styles.css --css ./dist/css/styles.css --content ./dist/en/index.html ./dist/en/album/index.html",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js",
        "deploy": "rsync -av --delete --exclude 'rsvp.csv' --exclude '.well-known' dist/ speedypage:matthewandmaria.com"
    },
    "description": "A grayscale one page HTML theme for Bootstrap.",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://startbootstrap.com/theme/grayscale",
    "bugs": {
        "url": "https://github.com/StartBootstrap/startbootstrap-grayscale/issues",
        "email": "feedback@startbootstrap.com"
    },
    "license": "MIT",
    "author": "Start Bootstrap",
    "contributors": [
        "David Miller (https://davidmiller.io/)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/StartBootstrap/startbootstrap-grayscale.git"
    },
    "dependencies": {
        "bootstrap": "^5.3.7"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.21",
        "browser-sync": "^3.0.4",
        "chokidar": "^4.0.3",
        "concurrently": "^9.2.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "pug": "^3.0.3",
        "purgecss": "^7.0.2",
        "sass": "^1.90.0",
        "shelljs": "^0.10.0",
        "uglify-js": "^3.19.3",
        "upath": "^2.0.1"
    }
}
